{
 "Name": "ICA",
 "Path": "AnyWAvePath/AnyWave.exe",
 "Parameters": {
  "Mode": [
   "automatic"
  ],
  "Intermediate": "AnyWave",
  "Callname": "ica",
  "hp": {
   "Default": "0",
   "Unit": "Hz"
  },
  "lp": {
   "Default": "0",
   "Unit": "Hz"
  },
  "comp": {
   "Default": "",
   "Unit": "components"
  },
  "downsampling": {
   "Default": true,
   "InCommandLine": true
  },
  "modality": {
   "ReadBids": true,
   "Type": "modality"
   },
  "use_markers": {
   "Read": "*.mrk",
   "elementstoread": "0",
   "multipleselection": true
  },
  "skip_markers": {
   "read": "*.mrk",
   "ElementsToRead": "0",
   "MultipleSelection": true
  },
  "skip_bad": {
    "Default": true,
    "InCommandLine": true
  },
  "Input": [
   {
    "Tag": "--input_file",
	"MultipleSubject": false,
    "Modality": [
     "Ieeg", "Meg", "Eeg"
    ],
    "Type": "file"
   }
  ],
  "Output": {
   "Tag": "--output_dir",
   "MultipleSubject": false,
   "Directory": true,
   "Type": "",
   "Extension": ".mat"
  }
 }
}